<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 
	2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> -->
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>chinapubcf</display-name>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/chinapubcf</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <servlet-name>user-recommander</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.UserBasedRecommanderServlet</servlet-class>
    <init-param>
      <param-name>userrecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.UserBasedRecommander</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>item-recommander</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.ItemBasedRecommanderServlet</servlet-class>
    <init-param>
      <param-name>itemrecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.ItemBasedRecommander</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>slope-recommander</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.SlopeOneRecommanderServlet</servlet-class>
    <init-param>
      <param-name>sloperecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.SlopeOneRecommander</param-value>
    </init-param>
  </servlet>
  <servlet>
  	<servlet-name>chinapub</servlet-name>
  	<servlet-class>chinapub.cf.recommander.servlet.ChinapubRecommanderServlet</servlet-class>
  	<init-param>
      <param-name>itemrecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.ItemBasedRecommander</param-value>
    </init-param>
    <init-param>
      <param-name>userrecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.UserBasedRecommander</param-value>
    </init-param>
    <init-param>
      <param-name>sloperecommander</param-name>
      <param-value>chinapub.cf.recommander.recommander.SlopeOneRecommander</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>books</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.BookServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>booklist</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.BookListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>rating</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.RatingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>sessionlogin</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.SessionLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>axis</servlet-name>
    <servlet-class>org.apache.axis.transport.http.AxisServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>item-recommander</servlet-name>
    <url-pattern>/itemrecmd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>slope-recommander</servlet-name>
    <url-pattern>/sloperecmd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>user-recommander</servlet-name>
    <url-pattern>/userrecmd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
 	 <servlet-name>chinapub</servlet-name>
 	 <url-pattern>/chinapub</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>books</servlet-name>
    <url-pattern>/books</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>booklist</servlet-name>
    <url-pattern>/booklist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>rating</servlet-name>
    <url-pattern>/rating</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sessionlogin</servlet-name>
    <url-pattern>/sessionlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>axis</servlet-name>
    <url-pattern>*.jws</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
  <mime-mapping>
    <extension>wsdl</extension>
    <mime-type>text/xml</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>xsd</extension>
    <mime-type>text/xml</mime-type>
  </mime-mapping>
  <servlet>
    <description>注册sseervlet</description>
    <display-name>RegisterServlet</display-name>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RatingServlet</display-name>
    <servlet-name>RatingServlet</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.RatingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RatingServlet</servlet-name>
    <url-pattern>/RatingServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>SessionLoginServlet</display-name>
    <servlet-name>SessionLoginServlet</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.SessionLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionLoginServlet</servlet-name>
    <url-pattern>/SessionLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ChinapubRecommanderServlet</display-name>
    <servlet-name>ChinapubRecommanderServlet</servlet-name>
    <servlet-class>chinapub.cf.recommander.servlet.ChinapubRecommanderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChinapubRecommanderServlet</servlet-name>
    <url-pattern>/ChinapubRecommanderServlet</url-pattern>
  </servlet-mapping>
</web-app>